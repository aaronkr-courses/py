{
  "name": "Python Jupyter ML",
  "image": "mcr.microsoft.com/devcontainers/python:3.11",
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-toolsai.jupyter",
        "ms-python.python",
        "ms-toolsai.jupyter-keymap",
        "ms-toolsai.jupyter-renderers",
        "ms-python.vscode-pylance"
      ]
    }
  },
  "postCreateCommand": "pip install --upgrade pip && pip install ipykernel && pip install -r requirements.txt",
  "remoteUser": "vscode"
}
